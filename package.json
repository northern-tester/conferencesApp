    "cross-env": "^5.0.5",
    "dotenv-safe": "^4.0.3",
    "eslint": "^8.2.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "jest-cli": "^27.3.1",
    "mongodb-memory-server": "^1.7.3",
    "nock": "^9.0.2",
    "nodemon": "^2.0.4",
    "opn-cli": "^1.0.0",
    "sinon": "^4.0.1",
    "supertest": "^3.0.0"
